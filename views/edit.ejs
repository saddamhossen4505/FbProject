

 <div class="profile-body">

    <div class="profile-wraper">
        <div class="profile-info">
            <%- include ('layouts/userInfo.ejs') %>
            <hr>
            <%- include ('layouts/menu.ejs') %> 
        </div>
        <div class="profile-box"> 
            <div class="box-content">
               <h2>Edit-UserData</h2>
               <% if (message) { %>
                   <p><%= message %> </p>
               <% } %>
                <hr>
                <div class="form-wraper">
                    <form action="/user/edit" method="POST">
                        <div class="form-group">
                            <label for="">Name</label>
                            <input name="name" value="<%= name %> " type="text">
                        </div>
                        <div class="form-group">
                            <label for="">Email</label>
                            <input name="email" value="<%= email %> " type="text">
                        </div>

                        <div class="form-group">
                            <label for="">Cell</label>
                            <input name="cell" value="<%= cell %> " type="text">
                        </div>

                        <div class="form-group">
                            <label for="">Location</label>
                            <input name="location" value="<%= location %> " type="text">
                        </div>

                        <div class="form-group">
                            <label for="">Gender</label>
                            <input name="gender" value="<%= gender %> " type="text">
                        </div>

                        <div class="form-group">
                            <label for="">Skill</label>
                            <input name="skill" value="<%= skill %> " type="text">
                        </div>

                        <div class="form-group">
                            <button type="submit" >Update-Now</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

</div>